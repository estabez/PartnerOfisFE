  },\r\n            {\r\n                data: \"project\",\r\n                label: \"Project\",\r\n                type: \"text\",\r\n                mandatory: false,\r\n                manually: true,\r\n                width: 100\r\n            }\r\n        ]\r\n    },\r\n    {\r\n        label: \"Information\",\r\n        color: \"info\",\r\n        colspan: 0,\r\n        columns: [\r\n            {\r\n                data: \"comments\",\r\n                label: \"Comments\",\r\n                type: \"text\",\r\n                mandatory: false,\r\n                manually: true,\r\n                width: 100\r\n            }\r\n        ]\r\n    },\r\n    {\r\n        label: \"Dates\",\r\n        color: \"dates\",\r\n        colspan: 0,\r\n        columns: [\r\n            {\r\n                data: \"insertionDate\",\r\n                label: \"Insertion Date\",\r\n                type: \"date\",\r\n                dateFormat: \"DD/MM/YYYY\",\r\n                datePickerConfig: {\r\n                    firstDay: 1\r\n                },\r\n                mandatory: false,\r\n                manually: true,\r\n                width: 120\r\n            },\r\n            {\r\n                data: \"lastModifiedDate\",\r\n                label: \"Last Modified Date\",\r\n                type: \"date\",\r\n                dateFormat: \"DD/MM/YYYY\",\r\n                datePickerConfig: {\r\n                    firstDay: 1\r\n                },\r\n                mandatory: false,\r\n                manually: true,\r\n                width: 140\r\n            },\r\n            {\r\n                data: \"archivedDate\",\r\n                label: \"Archived Date\",\r\n                type: \"date\",\r\n                dateFormat: \"DD/MM/YYYY\",\r\n                datePickerConfig: {\r\n                    firstDay: 1\r\n                },\r\n                mandatory: false,\r\n                manually: true,\r\n                width: 130\r\n            },\r\